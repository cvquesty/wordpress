{
  "name": "cvquesty-wordpress",
  "version": "0.1.0",
  "author": "cvquesty",
  "summary": "Puppet module to install and configure Wordpress",
  "license": "Apache-2.0",
  "source": "https://github.com/cvquesty/wordpress.git",
  "project_page": "https://github.com/cvquesty/wordpress",
  "issues_url": "https://github.com/cvquesty/wordpress/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ]
}
